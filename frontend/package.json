{
  "name": "mui-app-boilerplate",
  "author": "Marc-Olivier Laux <marc-olivier@matlaux.net> (https://github.com/molaux)",
  "description": "MUI App boilerplate",
  "version": "0.0.1",
  "homepage": "/",
  "homepage-electron": "./",
  "private": true,
  "main": "public/electron.js",
  "build": {
    "appId": "net.mui-admin-boilerplate.electron-mui-app-boilerplate",
    "productName": "Material-UI App Boilerplate",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "icon": "build/icons/icon.png",
      "target": [
        "AppImage"
      ],
      "category": "Utility"
    },
    "win": {
      "icon": "build/icons/icon.png",
      "target": [
        "nsis"
      ]
    }
  },
  "dependencies": {
    "@apollo/client": "^3.4.13",
    "@date-io/date-fns": "^2.11.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@molaux/mui-crudf": "^1.1.4",
    "@molaux/mui-utils": "^1.1.4",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.0.0-rc.0",
    "@mui/styles": "^5.0.1",
    "@svgr/webpack": "^5.5.0",
    "apollo-upload-client": "^16.0.0",
    "color": "<4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.23.0",
    "date-fns-tz": "^1.1.6",
    "dateformat": "^4.5.1",
    "deep-equal": "^2.0.5",
    "dotenv": "^10.0.0",
    "elasticsearch": "^16.7.2",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^15.6.0",
    "graphql-anywhere": "^4.2.7",
    "is-electron": "^2.2.0",
    "is-electron-renderer": "^2.0.1",
    "json5": "^2.2.0",
    "klona": "^2.0.4",
    "locutus": "^2.0.15",
    "pluralize": "^8.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-barcode": "^1.4.0",
    "react-barcode-reader": "^0.0.2",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0-next.47",
    "react-swipeable-views": "^0.14.0",
    "recharts": "^2.1.0",
    "seedrandom": "^3.0.5",
    "subscriptions-transport-ws": "^0.9.19",
    "typeface-roboto": "^1.1.13",
    "use-timeout": "^1.1.0",
    "uuid": "^8.3.2",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "scripts": {
    "start:desktop:dev": "cross-env NODE_ENV=dev nodemon --exec \"\"electron .\"\"",
    "prebuild:desktop": "sed -i -e s/\\\"homepage\\\"/\\\"homepage-react\\\"/ -e s/\\\"homepage-electron\\\"/\\\"homepage\\\"/ package.json ; yarn build-web ; sed -i -e s/\\\"homepage\\\"/\\\"homepage-electron\\\"/ -e s/\\\"homepage-react\\\"/\\\"homepage\\\"/ package.json",
    "build:desktop": "yarn electron-builder build -c.extraMetadata.main=build/electron.js -c.extraMetadata.homepage-react=./ --publish never",
    "build:desktop:debug": "DEBUG=electron-builder yarn electron-builder build -c.extraMetadata.main=build/electron.js -c.extraMetadata.homepage-react=./ --publish never",
    "start:web:dev:secured": "HTTPS=true react-app-rewired start",
    "start:web:dev": "react-app-rewired start",
    "build": "sh -ac '. ./.env.${REACT_APP_ENV}; react-scripts build'",
    "build:web:production": "REACT_APP_ENV=production yarn build",
    "build:web:test": "REACT_APP_ENV=test yarn build",
    "test": "react-app-rewired test --env=jsdom",
    "prebuild:all:production": "npm --no-git-tag-version version patch",
    "build:all:production": "yarn build:web:production && yarn build:desktop",
    "prepublish:web:production": "rm -f build/{bundle-stats.json,bundle-stats.html}",
    "prepublish:web:test": "rm -f build/{bundle-stats.json,bundle-stats.html}",
    "publish:web:production": "rm -rf prod-build.last/{*,.htaccess} && mv prod-build/{*,.htaccess} prod-build.last/  && cp -r --preserve build/{*,.htaccess} prod-build/",
    "publish:web:test": "rm -rf test-build.last/{*,.htaccess} && mv test-build/{*,.htaccess} test-build.last/  && cp -r --preserve build/{*,.htaccess} test-build/",
    "eject": "react-scripts eject",
    "build-doc": "find -type f -name \"*.js\" | egrep -v \"./(prod-)?build\" | xargs dirname | uniq | xargs -n 1 ./build-doc.sh",
    "build:web:audit": "sh -ac '. ./.env.test; react-scripts build --stats && webpack-bundle-analyzer build/bundle-stats.json -m static -r build/bundle-stats.html -O'",
    "generate:assets": "node bin/build-icons.js"
  },
  "browserslist": [
    ">5%",
    "not dead",
    "not ie > 0",
    "not ie_mob > 0",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^13.2.1",
    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-app": "^6.2.2",
    "eslint-plugin-standard": "^5.0.0",
    "jsdoc-to-markdown": "^7.0.1",
    "nodemon": "^2.0.12",
    "react-app-rewired": "^2.1.8",
    "yarn-upgrade-all": "^0.5.4"
  },
  "resolutions": {
    "webpack": "^5.54.0"
  }
}
